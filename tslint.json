{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended", "tslint-react"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": false,
        "indent": [true, "tabs"],
        "object-literal-sort-keys": false,
        "jsx-wrap-multiline": false,
        "jsx-no-multiline-js": false,
        "member-ordering": false,
        "interface-name": false,
        "arrow-parens": false,
        "curly": false,
        "jsx-no-lambda": false,
        "jsx-boolean-value": false
    },
    "rulesDirectory": []
}
